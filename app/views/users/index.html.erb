<h1>注册页面</h1>
<%= form_tag do  %>
       <div class="name">
         <%= text_field_tag  :email , params[:email]%>
       </div>
        <div class="password">
         <%= password_field_tag  :password %>
        </div>
    <div class="action">
      <%= submit_tag "注册" %>
    </div>
<% end %>